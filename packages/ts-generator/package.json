{
  "name": "@ozhanefe/ts-codegenerator",
  "version": "1.7.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --minify --clean",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "bun run build && bun run lint && bun run test && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.12",
    "tsup": "^8.1.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "ts-morph": "^23.0.0",
    "typescript": "^5.0.0"
  },
  "tsup": {
    "external": [
      "ts-morph",
      "typescript"
    ]
  },
  "files": [
    "dist"
  ]
}
